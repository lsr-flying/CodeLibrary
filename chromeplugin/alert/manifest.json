{  
    "name": "alertPlugin",  
    "version": "1.0.1",  
    "manifest_version": 2,  
    "description": "alert practice",  
    "icons": {  
        "48": "img/alert_clock_48.png",
 	"32": "img/alert_clock_32.png",
 	"16": "img/alert_clock_16.png"
    },  
    "browser_action": {  
      "default_icon": "img/alert_32.png",  
      "default_popup": "page/alert.html"  
    },
    "permissions":[
       "notifications"
    ],
  "content_security_policy": "script-src 'self' https://cdn.bootcss.com; object-src 'self'"
}  